{
  // .NET
  "dotnet.server.useOmnisharp": false,
  "dotnet.formatting.organizeImportsOnFormat": true,
  // Breadcrumbs
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "on",
  "breadcrumbs.icons": true,
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showClasses": false,
  "breadcrumbs.showConstants": false,
  "breadcrumbs.showConstructors": false,
  "breadcrumbs.showEnumMembers": false,
  "breadcrumbs.showEnums": false,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showFields": false,
  "breadcrumbs.showFiles": false,
  "breadcrumbs.showFunctions": false,
  "breadcrumbs.showInterfaces": false,
  "breadcrumbs.showKeys": false,
  "breadcrumbs.showMethods": false,
  "breadcrumbs.showModules": false,
  "breadcrumbs.showNamespaces": false,
  "breadcrumbs.showNull": false,
  "breadcrumbs.showNumbers": false,
  "breadcrumbs.showObjects": false,
  "breadcrumbs.showOperators": false,
  "breadcrumbs.showPackages": false,
  "breadcrumbs.showProperties": false,
  "breadcrumbs.showStrings": false,
  "breadcrumbs.showStructs": false,
  "breadcrumbs.showTypeParameters": false,
  "breadcrumbs.showVariables": false,
  "breadcrumbs.symbolPath": "off",
  "breadcrumbs.symbolSortOrder": "position",
  // C#
  "csharp.format.enable": true,
  "csharp.suppressHiddenDiagnostics": false,
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  // CodeSnap
  "codesnap.shutterAction": "copy",
  // Code Spell Checker
  "cSpell.enabled": true,
  "cSpell.userWords": [
    "Alena",
    "browserslist",
    "contentful",
    "pluralsight",
    "reduxjs",
    "stylelint"
  ],
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    ".git",
    "*.dll",
    "**/*.dll"
  ],
  // CSS
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "css.trace.server": "off",
  "css.completion.completePropertyWithSemicolon": true,
  "css.completion.triggerPropertyValueCompletion": true,
  "css.hover.documentation": true,
  "css.lint.argumentsInColorFunction": "error",
  "css.lint.emptyRules": "warning",
  "css.lint.float": "error",
  "css.lint.hexColorLength": "error",
  "css.lint.duplicateProperties": "error",
  "css.lint.fontFaceProperties": "error",
  "css.lint.important": "warning",
  "css.lint.zeroUnits": "error",
  "[css]": {
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  // CSV
  "[csv]": {
    "files.insertFinalNewline": false
  },
  // Docker
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Editor
  "editor.multiCursorModifier": "alt",
  "editor.fontFamily": "IntelOne Mono",
  "editor.fontSize": 12,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": true,
  "editor.wordWrap": "on",
  "editor.smoothScrolling": false,
  "editor.showUnused": true,
  "editor.selectionHighlight": true,
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.tabCompletion": "on",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": true,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.scale": 1,
  "editor.minimap.side": "right",
  "editor.renderWhitespace": "boundary",
  "editor.renderControlCharacters": false,
  "editor.codeLens": true,
  "editor.codeLensFontFamily": "Roboto Mono",
  "editor.lineNumbers": "on",
  "editor.links": true,
  "editor.matchBrackets": "always",
  "editor.largeFileOptimizations": true,
  "editor.linkedEditing": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.markdownlint": "explicit"
  },
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.guides.indentation": true,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.minimap.showSlider": "always",
  "editor.occurrencesHighlight": "multiFile",
  "editor.inlayHints.enabled": "offUnlessPressed",
  // Emmet
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "django-html": "html",
    "javascript": "javascriptreact",
    "postcss": "css",
  },
  // ESLint
  "eslint.enable": true,
  "eslint.run": "onSave",
  "eslint.quiet": false,
  "eslint.format.enable": false,
  "eslint.trace.server": "off",
  // Explorer
  "explorer.confirmDelete": false,
  "explorer.decorations.badges": true,
  "explorer.enableDragAndDrop": true,
  "explorer.openEditors.visible": 10,
  "explorer.sortOrder": "default",
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.autoReveal": true,
  // Files
  "files.autoSave": "onWindowChange",
  "files.eol": "\n",
  "files.enableTrash": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/web/node_modules/**": true,
    "**/web/build": true,
    "**/admin/node_modules": true
  },
  // Git
  "git.mergeEditor": true,
  // GitLens
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.defaultTimeFormat": "HH:mm",
  "gitlens.defaultDateFormat": "MMMM Do, YYYY HH:mm",
  "gitlens.statusBar.enabled": false,
  "gitlens.telemetry.enabled": false,
  "gitlens.codeLens.scopes": [
    "document",
    "containers"
  ],
  "gitlens.graph.layout": "editor",
  // Go
  // Handlebars
  "[handlebars]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  // HTML
  "html.autoClosingTags": true,
  "html.format.enable": true,
  "html.format.wrapAttributes": "aligned-multiple",
  "html.hover.documentation": true,
  "html.suggest.html5": true,
  "[html]": {
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "vscode.html-language-features",
  },
  // Intellicode
  "vsintellicode.python.completionsEnabled": true,
  "vsintellicode.typescript.completionsEnabled": true,
  "vsintellicode.java.completionsEnabled": false,
  "vsintellicode.sql.completionsEnabled": true,
  // JavaScript/TypeScript
  "javascript.autoClosingTags": true,
  "javascript.referencesCodeLens.enabled": true,
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "prompt",
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.inlayHints.variableTypes.suppressWhenTypeMatchesName": true,
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "files.eol": "\n"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "files.eol": "\n"
  },
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.referencesCodeLens.enabled": false,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.suggest.autoImports": true,
  "typescript.suggest.completeJSDocs": true,
  "typescript.suggest.jsdoc.generateReturns": true,
  "typescript.surveys.enabled": false,
  "typescript.tsserver.enableTracing": false,
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "typescript.tsserver.log": "off",
  "typescript.tsserver.useSyntaxServer": "auto",
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  "typescript.validate.enable": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.variableTypes.suppressWhenTypeMatchesName": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "files.eol": "\n"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.insertSpaces": true,
    "files.eol": "\n"
  },
  // JSON
  "json.format.enable": true,
  "json.schemaDownload.enable": true,
  "json.trace.server": "off",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "json.schemas": [
    {
      "name": "vscode-expo-manifest",
      "url": "file:///home/serhii/.config/Code/User/globalStorage/bycedric.vscode-expo/manifest-44.0.0.json",
      "fileMatch": [
        "app.json",
        "app.config.json"
      ]
    }
  ],
  // Markdown
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Material Icon Theme
  "material-icon-theme.activeIconPack": "nest",
  // npm
  "npm.runSilent": false,
  "npm.autoDetect": "on",
  "npm.fetchOnlinePackageInfo": true,
  "npm.packageManager": "auto",
  // npm Intellisense
  "npm-intellisense.importES6": true,
  "npm-intellisense.importQuotes": "'",
  "npm-intellisense.importDeclarationType": "const",
  "npm-intellisense.recursivePackageJsonLookup": false,
  "npm-intellisense.scanDevDependencies": true,
  "npm-intellisense.showBuildInLibs": true,
  "npm-intellisense.importLinebreak": ";\n",
  // Python
  "python.analysis.completeFunctionParens": true,
  "python.analysis.autoImportCompletions": true,
  "python.languageServer": "Pylance",
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never"
    },
  },
  // SCM
  "scm.showIncomingChanges": "auto",
  "scm.showOutgoingChanges": "auto",
  // Security
  "security.workspace.trust.untrustedFiles": "open",
  // Solidity
  "solidity.formatter": "prettier",
  "solidity.linter": "solhint",
  // Stylelint
  "stylelint.enable": true,
  "stylelint.packageManager": "yarn",
  "stylelint.validate": [
    "css",
    "html",
    "less",
    "markdown",
    "postcss",
    "sass",
    "scss",
    "source.css.styled",
    "source.markdown.math",
    "styled-css",
    "sugarss",
    "svelte",
    "vue",
    "vue-html",
    "vue-postcss",
    "xml",
    "xsl",
    "typescriptreact",
    "javascriptreact"
  ],
  // Supermaven
  "supermaven.allowGitignore": false,
  "supermaven.otherWarning": true,
  // Svelte
  "svelte.enable-ts-plugin": true,
  "svelte.trace.server": "off",
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  // SVG
  "svg.preview.mode": "img",
  "svg.completion.emmet": true,
  "svg.completion.showAdvanced": false,
  "svg.completion.showDeprecated": false,
  "svg.preview.autoShow": true,
  "svg.preview.background": "transparent",
  "svg.preview.showRuler": true,
  "svg.preview.toolbarSize": "middle",
  "svg.preview.viewMode": "onlyOne",
  // TailwindCSS
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  },
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.colorDecorators": true,
  "tailwindCSS.hovers": true,
  "tailwindCSS.lint.cssConflict": "warning",
  "tailwindCSS.lint.invalidApply": "error",
  "tailwindCSS.lint.invalidScreen": "error",
  "tailwindCSS.validate": true,
  "tailwindCSS.showPixelEquivalents": true,
  "tailwindCSS.suggestions": true,
  "tailwindCSS.rootFontSize": 16,
  // Telemetry
  "telemetry.telemetryLevel": "off",
  // Terminal
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "MesloLGM Nerd Font",
  "terminal.integrated.fontSize": 10,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.tabs.focusMode": "singleClick",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.profiles.linux": {
    "zsh": {
      "path": "zsh",
      "icon": "terminal-ubuntu",
      "color": "terminal.ansiMagenta"
    },
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  // Window
  "window.titleBarStyle": "custom",
  "window.doubleClickIconToClose": false,
  "window.titleSeparator": " - ",
  "window.closeWhenEmpty": false,
  "window.commandCenter": false,
  // Workbench
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.colorTheme": "Default Dark+",
  "workbench.list.openMode": "singleClick",
  "workbench.editor.enablePreview": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.startupEditor": "welcomePage",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.tree.indent": 8,
  "workbench.colorCustomizations": {
    "statusBar.background": "#8b008b",
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  // YAML
  "redhat.telemetry.enabled": false,
  "yaml.trace.server": "off",
  "yaml.completion": true,
  "yaml.format.enable": true,
  "yaml.validate": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "docker.extension.enableComposeLanguageServer": false,
}
